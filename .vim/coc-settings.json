{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "graphql",
    "html",
    "yaml",
    "json"
  ],
  "snippets.utilsnips.directories": ["UltiSnips", "~/.vim/utils/snips"],
  "languaserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },
  "explor.icon.enableVimDevicons": true
}
